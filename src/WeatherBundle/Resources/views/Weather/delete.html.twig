{% extends "::base.html.twig" %}
{% block title %}Nowa strona {% endblock %}
{% block body %}
    <h1>Select city that You want to delete from weather city list:</h1>
    <form action="{{ asset('delete') }}" method="post">
    {% for city in cities %}
        <input type="radio" name="cityToDelete" value="{{ city.id }}">{{ city.name }}<br>
    {% endfor %}
        <input type="submit" value="Delete">
    </form>
{% endblock %}